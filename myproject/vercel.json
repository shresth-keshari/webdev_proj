{
        "version": 2,
        "builds": [
        {
            "src": "myproject/wsgi.py" ,
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        },
        {
            "src": "build files.sh",
            "use": "@vercel/static-build",
            "config":{
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$l"
        },
        { 
            "src": "/(.*)",
            "dest": "myproject/wsgi.py"
        }
    ]
}